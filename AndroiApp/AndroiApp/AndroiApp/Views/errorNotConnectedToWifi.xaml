<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AndroiApp.Views.errorNotConnectedToWifi">
    <ContentPage.Content>
        <StackLayout Orientation="Vertical" Padding="50,50,50,50">
            <Label Text="You are not connected to WIFI" />
            <Label Text="Connect and then try again"/>
            <Button x:Name="backButton" Text="Back"
                    BackgroundColor="{x:DynamicResource buttonBGColor}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>